{
    "plugins": [],
    "recurseDepth": 10,
    "opts": {
      "recurse": true
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?|.+\\.ts(x)?$",
      "excludePattern": "(^|\\/|\\\\)(base|node_modules)"
    },
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }